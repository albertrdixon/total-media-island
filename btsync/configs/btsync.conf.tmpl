{ 
  "device_name": "{{ default .Env.DEVICE_NAME "docker btsync" }}",
  "listening_port" : {{ default .Env.LISTEN_PORT 55555 }},
  "storage_path" : "/btsync/.sync",
  "check_for_updates" : true, 
  "use_upnp" : true,
  "download_limit" : 0,                       
  "upload_limit" : 0, 
  "webui" :
  {
    "listen" : "0.0.0.0:{{ default .Env.UI_PORT "8888" }}",
    "login" : "{{ default .Env.USERNAME "admin" }}",
    "password" : "{{ default .Env.PASSWORD "admin" }}"
  }
}
{{ path "/btsync" }}
